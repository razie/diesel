#
# MAKE A backup in production dB and get it here
#

DT=`date | sed 's/ //g'`

mv dump dump.OLD.$DT

ssh demo@cloud3.apefsjdolj.ro "cd ~/ilp1/app/run; . ./dbr-backup-prod"

rm dump.tar dump.tar.gz

scp demo@cloud3.apefsjdolj.ro:ilp1/app/run/dump.tar.gz ./dump.tar.gz

gunzip dump.tar.gz

tar xvf dump.tar

